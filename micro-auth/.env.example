# JWT Secrets (MUST be set in production; dev defaults are used if not set)
JWT_SECRET=dev-jwt-secret
REFRESH_SECRET=dev-refresh-secret

# Token Expiry Times
ACCESS_TOKEN_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

# Node Environment
NODE_ENV=development

# Port
PORT=5005

# Logging
LOG_LEVEL=info

# Redis Configuration (Optional - falls back to in-memory cache if not configured)
REDIS_HOST=172.31.78.234
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
